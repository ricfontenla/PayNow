<h1><%= t('.title') %></h1>
<hr class="my-4">

<%= form_with model: [:admin, @company, @order] do |form| %>
  <dl>
    <dt><%= form.label :status %></dt>
    <dd><%= form.select :status, [['Pendente', :pendente], ['Aprovado', :aprovado]] %></dd>

    <dt><%= form.label :response_code %></dt>
    <dd><%= form.select :response_code, @response_code %></dd>

    <%= form.submit t('.submit'), class: 'btn btn-dark' %>
  </dl>
<% end %>